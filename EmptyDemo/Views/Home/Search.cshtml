@model IEnumerable<Item>

@if (Model.Count() > 0)
{
    <p class="display-4">Results for your search</p>
    <hr />
    <div class="row">
        @foreach(var item in Model)
        {
            <partial name="_ItemPartial" model="item" />
        }
    </div>
}
else 
{
    <p class="display-4">No items found...</p>
}

